{
    "manifest_version": 3,
    "name": "Netflix Skipper Upgraded",
    "version": "2.0",
    "description": " skip Netflix intros, recap and jump to the next episode",
    "permissions": ["activeTab", "scripting", "storage", "tabs"],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "48": "icon.png"
      }
    },
    "icons": {
      "48": "icon.png"
    },
    "background": {
        "service_worker": "background.js"
    }
}
  